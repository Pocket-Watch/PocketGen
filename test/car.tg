type Car {
    #Numbers
    u32 yearProduced; u32 hp; u16 doors;
    #Details
    const Details details;

    func drive() ;
    func break() i64;
}

type Details {
    const string brand;
    #Numbers
    u64 mileage; i8 gears; f64 fuelConsumption;
    [string] features;

    func display() string;
}
